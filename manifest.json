
{
  "manifest_version": 3,
  "name": "philosopher's battlefield",
  "version": "1.0",
  "description": "An interactive Chrome extension.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": "./asset/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["asset/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
